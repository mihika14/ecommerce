{
    "version": 2,
    "routes": [
      { "handle": "filesystem" },
      { "src": "/createuser", "dest": "/backend/main.js" },
      { "src": "/loginuser", "dest": "/backend/main.js" },
      { "src": "/cartitems", "dest": "/backend/main.js" },
      { "src": "/cartitems/:id", "dest": "/backend/main.js" },
      { "src": "/additem", "dest": "/backend/main.js" },
      { "src": "/(.*)", "dest": "/backend/main.js" }
    ],
    "builds": [
      {
        "src": "/(.*)",
        "use": "@vercel/node"
      }
    ]
  }
  